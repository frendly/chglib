  "license": "MIT",
  "scripts": {
    "start": "yarn dev:assets && yarn dev:html",
    "watch": "yarn start",
    "dev:html": "eleventy --serve --watch",
    "dev:assets": "webpack --mode development --watch",
    "dev:html_debug": "DEBUG=Eleventy:Benchmark* eleventy",
    "build:assets": "webpack --mode production",
    "build": "yarn build:assets && eleventy",
    "gzip": "gulp gzip",
    "deploy": "yarn gzip && cat ./dist/dist.tar.gz | ssh chglib@web.icp.ac.ru 'tar xzf - -C ~/.'"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "mini-css-extract-plugin": "^1.5.0",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.35.1",
    "webpack-cli": "^4.6.0",
    "webpack-manifest-plugin": "^3.1.1",
    "yaml-loader": "^0.6.0"
  },
  "dependencies": {
    "dayjs": "^1.10.4",
    "jquery": "^3.6.0"
  }
}
