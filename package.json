    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "css-loader": "^6.7.1",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-gzip": "^1.4.2",
    "gulp-include": "^2.4.1",
    "gulp-manifest-replace": "^1.0.1-rc.1",
    "gulp-pretty-html": "^2.0.10",
    "gulp-sass": "^5.1.0",
    "gulp-tar": "^2.1.0",
    "gulp-yaml": "^2.0.4",
    "mini-css-extract-plugin": "^2.6.1",
    "require-dir": "^1.2.0",
