{
  "name": "chglib",
  "version": "1.1.0",
  "main": "index.js",
  "repository": "git@bitbucket.org:frendly/chglib.git",
  "author": "frendly <www.zpdf@mail.ru>",
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf ./dist dist.tar.gz",
    "start": "yarn watch",
    "gzip": "yarn build && tar -czf dist.tar.gz -C dist .",
    "deploy": "yarn gzip && cat ./dist.tar.gz | ssh chglib@web.icp.ac.ru 'tar xzf - -C ~/.'",
    "dev:html": "ELEVENTY_ENV=development eleventy --serve --watch",
    "dev:assets": "webpack --mode development",
    "build:assets": "webpack --mode production",
    "build:html": "ELEVENTY_ENV=production eleventy",
    "watch": "yarn clear && yarn dev:html",
    "build": "yarn clear && yarn build:assets && yarn build:html"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "css-loader": "^6.7.1",
    "esbuild": "^0.14.48",
    "esbuild-plugin-yaml": "^0.0.1",
    "esbuild-sass-plugin": "^2.2.6",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-manifest-plugin": "^5.0.0",
    "yaml-loader": "^0.8.0"
  },
  "dependencies": {
    "moment": "^2.24.0"
  }
}
