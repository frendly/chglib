{
  "name": "chglib",
  "version": "3.0.0",
  "main": ".eleventy.js",
  "type": "module",
  "repository": "git@github.com:frendly/chglib.git",
  "author": "frendly <www.zpdf@mail.ru>",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "clear": "rm -rf ./dist",
    "start": "yarn watch",
    "biome:format": "biome format --write",
    "biome:lint": "biome lint --write",
    "biome:check": "biome check --write",
    "watch": "yarn clear && eleventy --serve --watch",
    "build": "yarn clear && NODE_ENV=production eleventy",
    "deploy": "yarn build && yarn transfer",
    "transfer": "env-cmd -x rsync -az ./dist/ \\$SSH_HOST:\\$SSH_PATH"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@biomejs/biome": "2.1.4",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.25.0",
    "esbuild-yaml": "^2.0.0",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-custom-media": "^10.0.4",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.2.0"
  },
  "dependencies": {
    "dayjs": "^1.11.6"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
